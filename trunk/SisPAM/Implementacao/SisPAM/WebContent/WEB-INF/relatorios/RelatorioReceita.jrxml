	<queryString>
		<![CDATA[SELECT
     USUARIO.USRNOM,
     HISTORICOPRONTUARIO.HTCCOD,
     USUARIO.USRSEX,
     USUARIO.USRDATNSC,
     PACIENTE.PCTACM,
     CONVENIO.CVNDES,
     HISTORICOPRONTUARIO.HTCPRC,
     MEDICO.MDCCOD
FROM
     USUARIO,
     HISTORICOPRONTUARIO,
     PACIENTE,
     CONVENIO,
     AGENDAMENTO,
     MEDICO
WHERE
     USUARIO.USRCOD = PACIENTE.USRCOD
 AND AGENDAMENTO.PCTIDFSEG = HISTORICOPRONTUARIO.PCTIDFSEG
 AND AGENDAMENTO.MDCCOD = MEDICO.MDCCOD
 AND CONVENIO.CVNCOD = PACIENTE.CVNCOD
 AND PACIENTE.PCTIDFSEG = $P{PCTIDFSEG}
 AND AGENDAMENTO.AGDDAT = $P{AGDDAT}
 AND AGENDAMENTO.AGDHOR = $P{AGDHOR}]]>
	</queryString>
